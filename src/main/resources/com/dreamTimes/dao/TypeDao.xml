<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dreamTimes.dao.TypeDao">
    <select id="selectAll" resultType="com.dreamTimes.beans.Type">
        select * from typetable
    </select>

    <delete id="deleteByTname">
        delete from typetable where tname = #{tname}
    </delete>
</mapper>